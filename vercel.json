{
  "version": 2,
  "builds": [{ "src": "api/*.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "^/$", "status": 308, "headers": { "Location": "/api" } },
    { "src": "^/api$", "dest": "/api/index.js" },
    { "src": "^/api/health$", "dest": "/api/index.js" },
    { "src": "^/api/hello$", "dest": "/api/index.js" },
    { "src": "^/api/(.*)$", "dest": "/api/$1" }
  ]
}
